{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://fozilbek.dev/$1",
      "has": [
        {
          "type": "host",
          "value": "fozilbek.vercel.app"
        }
      ],
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
